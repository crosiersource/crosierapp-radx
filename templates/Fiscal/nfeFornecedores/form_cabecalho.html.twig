<div class="tab-pane fade show active" id="cabecalho" role="tabpanel" aria-labelledby="cabecalho-tab">

    <div class="container" style="margin-bottom: 50px">


        <div class="row">
            <div class="col-md-7 col-sm-12">
                <div class="form-group">
                    <label for="nota_fiscal_emitente">Emitente</label>
                    <input type="text" id="nota_fiscal_emitente" name="nota_fiscal[emitente]" disabled="disabled"
                           class="form-control"
                           value="{{ notaFiscal.documentoEmitente|mascararCnpjCpf }} - {{ notaFiscal.xNomeEmitente }}">
                </div>
            </div>

            <div class="col-md-3 col-sm-12">
                {{ form_row(form.numero) }}
            </div>
            <div class="col-md-2 col-sm-12">
                {{ form_row(form.serie) }}
            </div>
        </div>

        <div class="row">
            <div class="col-md-8 col-sm-12">
                {{ form_row(form.chaveAcesso) }}
            </div>
            <div class="col-md-4 col-sm-12">
                {{ form_row(form.protocoloAutorizacao) }}
            </div>
        </div>


        <div class="row">
            <div class="col-md-12">
                {{ form_row(form.naturezaOperacao) }}
            </div>
        </div>

        <div class="card">
            <div class="card-body">
                <h5 class="card-title">Destinatário/Remetente</h5>

                <div class="row">
                    <div class="col-md-9 col-sm-12">
                        <div class="form-group">
                            <label for="nota_fiscal_emitente">Destinatário</label>
                            <input type="text" id="nota_fiscal_emitente" name="nota_fiscal[emitente]"
                                   disabled="disabled"
                                   class="form-control"
                                   value="{{ notaFiscal.documentoDestinatario|mascararCnpjCpf }} - {{ notaFiscal.xNomeDestinatario }}">
                        </div>
                    </div>

                    <div class="col-md-3 col-sm-12">
                        {{ form_row(form.dtEmissao) }}
                    </div>

                </div>

            </div>
        </div>

    </div>


</div>

